{
  "kind": "collectionType",
  "collectionName": "authors",
  "info": {
    "singularName": "author",
    "pluralName": "authors",
    "displayName": "author"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Male",
        "Female"
      ]
    },
    "books": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::book.book",
      "mappedBy": "authors"
    }
  }
}
